{
  "manifest_version": 3,
  "name": "Smart AI Assistant",
  "version": "1.1",
  "description": "Floating Gemini Nano assistant overlay.",
  "permissions": ["scripting", "activeTab", "storage"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Smart AI Assistant",
    "default_icon": {
      "128": "icon128.png"
    }
  },
  "icons": {
    "128": "icon128.png"
  }
}
